      <FilesFilter>
        <FilesExtension>.src</FilesExtension>
        <FilesExtension>.dat</FilesExtension>
        <ContainName>Spot</ContainName>
        <ContainName>Stud</ContainName>
        <ContainName>Glue</ContainName>
        <ContainName>Pick</ContainName>
        <ContainName>Drop</ContainName>
        <ContainName>Center</ContainName>
        <ContainName>Arc</ContainName>
        <ContainName>Clinch</ContainName>
        <ContainName>Flow</ContainName>
        <ContainName>Rivet</ContainName>
        <ContainName>Nut</ContainName>
        <ContainName>Fasten</ContainName>
        <ContainName>Hem</ContainName>
        <ContainName>Laser</ContainName>
        <ContainName>Search</ContainName>
        <ContainName>Stack</ContainName>
        <ContainName>Spray</ContainName>
        <ContainName>Fit</ContainName>
        <ContainName>Inline</ContainName>
        <ContainName>Check</ContainName>
        <ContainName>Stamp</ContainName>
        <ContainName>Scan</ContainName>
        <ContainName>Vrtfx</ContainName>
      </FilesFilter>
      <DestinationFolder>Program</DestinationFolder>
      <ProgramType>Program</ProgramType>
    </FilesToCopy>
    <FilesToCopy>
      <FilesFilter>
        <FilesExtension>.src</FilesExtension>
        <FilesExtension>.dat</FilesExtension>
        <ContainName>Maintenan</ContainName>
        <ContainName>Dock</ContainName>
        <ContainName>Braketest</ContainName>
        <ContainName>Change</ContainName>
        <ContainName>Dress</ContainName>
        <ContainName>Tch_Auto</ContainName>
      </FilesFilter>
      <DestinationFolder>BMW_Std_User</DestinationFolder>
      <ProgramType>Service</ProgramType>
    </FilesToCopy>

    <DataToCopy>
      <FilesFilter>
        <FilesExtension>.olp</FilesExtension>
        <ContainName>Maintenan</ContainName>
        <ContainName>Dock</ContainName>
        <ContainName>Braketest</ContainName>
        <ContainName>Change</ContainName>
        <ContainName>Dress</ContainName>
        <ContainName>Tch_Auto</ContainName>
      </FilesFilter>
      <DestinationFolder>OLP</DestinationFolder>
      <FileType>Olp</FileType>
    </DataToCopy>
    <DataToCopy>
      <FilesFilter>
        <FilesExtension>_global.dat </FilesExtension>
        <ContainName>Maintenan</ContainName>
        <ContainName>Dock</ContainName>
        <ContainName>Braketest</ContainName>
        <ContainName>Change</ContainName>
        <ContainName>Dress</ContainName>
        <ContainName>Tch_Auto</ContainName>
      </FilesFilter>
      <DestinationFolder>OLP</DestinationFolder>
      <FileType>Global</FileType>
    </DataToCopy>

    <FilesToRemove>
      <FilesFilter>
        <FilesExtension>.log</FilesExtension>
      </FilesFilter>
    </FilesToRemove>
  </Controler>

</ControlersConfiguration>


   

  

